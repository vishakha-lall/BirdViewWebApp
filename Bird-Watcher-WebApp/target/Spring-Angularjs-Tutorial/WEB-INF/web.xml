<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Bird Watcher Web App</display-name>
  <servlet>
   <servlet-name>SpringAngular</servlet-name>
   <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
  </servlet>
  
  <servlet>
   <servlet-name>UserDispatch</servlet-name>
   <servlet-class>com.birdwatcher</servlet-class>
  </servlet>
  
  <servlet-mapping>
   <servlet-name>SpringAngular</servlet-name>
   <url-pattern>/SpringAngular</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
   <servlet-name>UserDispatch</servlet-name>
   <url-pattern>/</url-pattern>
  </servlet-mapping>
  
  <!-- <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list> -->
</web-app>